{
  "name": "mapifest-schema",
  "main": "dist/index.js",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "build": "rm -r dist 2>/dev/null; tsc",
    "dev": "npm run build -- -w",
    "lint": "eslint .",
    "test": "tsx test",
    "preversion": "npm run build && npm run lint && npm run test",
    "postversion": "npm pack",
    "postinstall": "if [ ! -h .git/hooks/pre-commit ]; then ln -vs ../../git-hooks/pre-commit .git/hooks/pre-commit; fi"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.33.12",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^8.57.1",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "tsx": "^4.19.1",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist/"
  ]
}
